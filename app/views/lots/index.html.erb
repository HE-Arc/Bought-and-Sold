<div class="page-header" id="banner">
  <div class="row">
    <div class="col-lg-8 col-md-7 col-sm-6">
      <h1>Batches</h1>
      <p class="lead">Your current profit : <%= @profit %> $</p>
      <p><%= link_to 'New', new_lot_path,:class=>"btn btn-primary" %>
      </p>
    </div>
  </div>
</div>

<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">List of all your batches</h3>
  </div>
  <div class="panel-body">
    <div id="shieldui-grid1"></div>
  </div>
</div>

<div class="page-header" id="banner">
  <div class="row">
    <div class="col-lg-6 col-md-5 col-sm-4">
      <h2>Add categories</h2>
        <div class="form-group">
          <% @categorie = Categorie.new %>
          <%= form_for(@categorie) do |f| %>
          <div class="navbar-form navbar-left">
					
          <%= f.text_field :name,:placeholder=>"Add your categorie",:class=>"form-control" %>
          <%= f.submit :class=>"btn btn-warning" %>
          </div>
          <% end %>
        </div>
    </div>
  </div>
</div>

